#include<iostream>
using namespace std;
int main() {

      int w[1000],v[1000],size;

     int num;
     cin>>num>>size;
     
     int t[num+1][k+1];

     for( int i=0;i<num;i++){

          cin>>w[i]>>v[i];

     }

     for( int i=0;i<size;i++){

            t[0][i]=0;
     }

    for( int i=0;i<num;i++){

            t[num][0]=0;
     }

     for( i=1;i<num;i++){
       
          for( j=1;j<size;j++){

              if(j<w[i]){
               // if the num are not in range
               // just print the upper previous cell 
               t[i][j] =t[i-1][j];
              }
              else {
                t[i][j]= max(t[i-1][j-w[i]]+v[i],t[i-1][j]);
              }
          }

     }

     for( int i=0;i<num;i++){

         for( int j=0;j<size;j++){

               cout<<t[i][j]<<" ";
         }

         cout<<endl;
     }


}
